<!DOCTYPE html>
<html>
  <head>
    <title>One</title>

    <% include include/game.ejs %>      
    
    <script>
      
      // Once user script is loaded, do whatever else needs to be done
      function userScriptLoaded() {     
        //gameStarted(initGame)
        playerHitEnergy(function(object) {
          hitEnergyEvent();
          removeObject(object);
        });     
        playerHitWall(hitWallEvent);
        playerHitMonster(hitMonsterEvent);
        playerHitGoal(goalReachedEvent)
        createRandomMaze(16,8)           
        playerStepTaken (function () {})           
      }      
      
      document.addEventListener('DOMContentLoaded', function() {

        // Load the user script file
        loadUserScript('/js/collision-events.js')        
        
      })

            
    </script>
    
  </head>
  <body>
    <div id="game"></div>
  </body>
</html>
